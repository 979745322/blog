<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rex.blog.mapper.AccessMapper">

    <!-- 新增来访记录 -->
    <insert id="addAccess">
        insert into tb_access(
        ACCESS_TIME,
        ACCESS_IP
        )values (
        now(),
        #{ip}
        )
    </insert>

</mapper>